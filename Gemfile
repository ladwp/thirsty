source 'https://rubygems.org'

gem 'rails', '~> 3.2.16'

# Bundle edge Rails instead:
# gem 'rails', :git => 'git://github.com/rails/rails.git'

gem 'pg'
gem 'nokogiri'
gem 'newrelic_rpm'

gem 'sass-rails'

# Sass was exploding when using the newest version, so pegging it to the last
# known working one.
gem 'bootstrap-sass', '3.1.0.2'
gem 'coffee-rails'
gem 'jquery-rails'
gem 'underscore-rails'
gem 'backbone-rails'
gem 'momentjs-rails'
gem 'd3-rails'
gem 'uglifier'

group :test do
  gem 'rspec-rails'
  gem 'webmock'
  gem 'spork'
  gem 'guard-rspec'
  gem 'factory_girl_rails'
end

group :test, :development do
  gem 'jasmine-rails'
  gem 'pry'
  gem 'test-unit'
end




